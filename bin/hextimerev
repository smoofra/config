#!/usr/bin/perl -w

%t = (
      '0' => '0',
      '1' => '8',
      '2' => '4',
      '3' => 'c',
      '4' => '2',
      '5' => 'a',
      '6' => '6',
      '7' => 'e',
      '8' => '1',
      '9' => '9',
      'a' => '5',
      'b' => 'd',
      'c' => '3',
      'd' => 'b',
      'e' => '7',
      'f' => 'f'
     );


while (<>) {
  for (split '', $_) {
    unshift @o, $t{$_} if /[a-fA-F0-9]/
  }
}

print join '', @o;

print "\n";



